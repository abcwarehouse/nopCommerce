@model AbcWarehouse.Plugin.Widgets.PriceSpider.Models.PriceSpiderModel

@{
    var productContent = string.Empty;

    foreach (var product in Model.Products)
    {
        productContent += product.ToString();
    }
}

<!-- PriceSpider -->
<img src='https://gethatch.com/iceleads_rest/merch/@Model.MerchantId/direct;@productContent' height='0' width='0'/>