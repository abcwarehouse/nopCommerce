@model AbcWarehouse.Plugin.Widgets.UniFi.Models.UniFiModel

<!-- UniFi -->
<!-- AbcItemNumber: @Model.AbcItemNumber -->
<script>
    var syfWidgetObject={};
    syfWidgetObject.syfPartnerId = "@Model.PartnerId";
    @if (!string.IsNullOrWhiteSpace(@Model.FlowType))
    {
        <text>syfWidgetObject.flowType = "@Model.FlowType";</text>
    }
    @if (!string.IsNullOrWhiteSpace(@Model.Tags))
    {
        <text>syfWidgetObject.tags = "@Model.Tags";</text>
    }
</script>
<script id=”syfMPPScript” type="text/javascript" src="@Model.Url"></script>